{
  "name": "vscode-taskwarrior",
  "license": "See License in README.md",
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/vscode": "^1.66.0",
    "@yarnpkg/sdks": "^3.0.0-rc.4",
    "eslint": "^8.18.0",
    "typescript": "^4.6.3",
    "vsce": "^2.9.2"
  },
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "compile": "tsc -p extension/",
    "login": "vsce login claui",
    "package": "cd extension && vsce package --no-dependencies --yarn --githubBranch main/extension",
    "postinstall": "yarn sdks vscode",
    "publish": "cd extension && vsce publish --no-dependencies --yarn --githubBranch main/extension",
    "_revert-selfupgrade-fallout": "git checkout -- .vscode/settings.json '.yarn/*/LICENSE'",
    "upgrade-all": "yarn upgrade-yarn-itself && yarn upgrade-packages",
    "upgrade-packages": "yarn up '*' && yarn _revert-selfupgrade-fallout",
    "upgrade-yarn-itself": "yarn set version latest && yarn _revert-selfupgrade-fallout",
    "vscode:prepublish": "yarn compile"
  }
}
